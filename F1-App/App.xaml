<Application x:Class="F1_App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:F1_App"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="Formula1 Display-Black">/F1-App;component/Fonts/#Formula1 Display Black</FontFamily>
        <FontFamily x:Key="Formula1 Display-Italic">/F1-App;component/Fonts/#Formula1 Display Italic</FontFamily>
        <FontFamily x:Key="Formula1 Display-Bold">/F1-App;component/Fonts/#Formula1 Display Bold</FontFamily>
        <FontFamily x:Key="Formula1 Display-Regular">/F1-App;component/Fonts/#Formula1 Display Regular</FontFamily>
    </Application.Resources>
</Application>
